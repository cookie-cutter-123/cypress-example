{
  "name": "cypress-example",
  "version": "1.0.0",
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.32.0",
    "cypress": "^12.17.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-prettier": "^5.3.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "cypress:open:it": "npx cypress open --env configFile=it",
    "cypress:open:pta": "npx cypress open --env configFile=pta",
    "cypress:run:it": "npx cypress run --env configFile=it",
    "cypress:run:pta": "npx cypress run --env configFile=pta",
    "lint:check": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix"
  }
}
